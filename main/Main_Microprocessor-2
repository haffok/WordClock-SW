        ledString[[8],[8]] = 1;
        ledString[[8],[9]] = 1;
        ledString[[8],[10]] = 1;  
     }//end method setHrZWOLF
     
     void setTenZERO(){
      ledString[[1],[2]] = 1;
      ledString[[2],[1]] = 1;
      ledString[[2],[3]] = 1;
      ledString[[3],[0]] = 1;
      ledString[[3],[4]] = 1;
      ledString[[4],[0]] = 1;
      ledString[[4],[4]] = 1;
      ledString[[5],[0]] = 1;
      ledString[[5],[4]] = 1;
      ledString[[6],[0]] = 1;
      ledString[[6],[4]] = 1;
      ledString[[7],[1]] = 1;
      ledString[[7],[3]] = 1;
      ledString[[8],[2]] = 1;
     }//end method setTenZERO
     
     void setTenONE(){
      ledString[[1],[4]] = 1;
      ledString[[2],[3]] = 1;
      ledString[[2],[4]] = 1;
      ledString[[3],[2]] = 1;
      ledString[[3],[4]] = 1;
      ledString[[4],[1]] = 1;
      ledString[[4],[4]] = 1;
      ledString[[5],[0]] = 1;
      ledString[[5],[4]] = 1;
      ledString[[6],[4]] = 1;
      ledString[[7],[4]] = 1;
      ledString[[8],[4]] = 1;
     }//end method setTenONE
 
      void setTenTWO(){
      ledString[[1],[2]] = 1;
      ledString[[2],[1]] = 1;
      ledString[[2],[3]] = 1;
      ledString[[3],[0]] = 1;
      ledString[[3],[4]] = 1;
      ledString[[4],[4]] = 1;
      ledString[[5],[3]] = 1;
      ledString[[6],[2]] = 1;
      ledString[[7],[1]] = 1;
      ledString[[8],[0]] = 1;
      ledString[[8],[1]] = 1;
      ledString[[8],[2]] = 1;
      ledString[[8],[3]] = 1;
      ledString[[8],[4]] = 1;
     }//end method setTenTWO
     
          void setTenTHREE(){
      ledString[[1],[0]] = 1;
      ledString[[1],[1]] = 1;
      ledString[[1],[2]] = 1;
      ledString[[2],[3]] = 1;
      ledString[[2],[4]] = 1;
      ledString[[3],[2]] = 1;
      ledString[[3],[3]] = 1;
      ledString[[4],[0]] = 1;
      ledString[[4],[1]] = 1;
      ledString[[5],[2]] = 1;
      ledString[[5],[3]] = 1;
      ledString[[6],[4]] = 1;
      ledString[[7],[2]] = 1;
      ledString[[7],[3]] = 1;
      ledString[[8],[0]] = 1;
      ledString[[8],[1]] = 1;
     }//end method setTenTHREE
     
          void setTenFOUR(){
      ledString[[1],[3]] = 1;
      ledString[[2],[2]] = 1;
      ledString[[2],[3]] = 1;
      ledString[[3],[1]] = 1;
      ledString[[3],[3]] = 1;
      ledString[[4],[1]] = 1;
      ledString[[4],[3]] = 1;
      ledString[[5],[0]] = 1;
      ledString[[5],[3]] = 1;
      ledString[[6],[0]] = 1;
      ledString[[6],[1]] = 1;
      ledString[[6],[2]] = 1;
      ledString[[6],[3]] = 1;
      ledString[[6],[4]] = 1;
      ledString[[7],[3]] = 1;
      ledString[[8],[3]] = 1;
     }//end method setTenFOUR
     
          void setTenFIFE(){
      ledString[[1],[0]] = 1;
      ledString[[1],[1]] = 1;
      ledString[[1],[2]] = 1;
      ledString[[1],[3]] = 1;
      ledString[[1],[4]] = 1;
