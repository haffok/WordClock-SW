      ledString[[3],[0]] = 1;
      ledString[[4],[0]] = 1;
      ledString[[4],[1]] = 1;
      ledString[[5],[2]] = 1;
      ledString[[5],[3]] = 1;
      ledString[[6],[4]] = 1;
      ledString[[7],[2]] = 1;
      ledString[[7],[3]] = 1;
      ledString[[8],[0]] = 1;
      ledString[[8],[1]] = 1;      
     }//end method setTenFIFE
     
