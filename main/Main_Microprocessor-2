#include <Utils.h>
#include <DCF77.h>
#include <Time.h>

//for Temperature-Sensor
#include <Wire.h>
#include <DS1631.h>
#include <Arduino.h>
#include <inttypes.h>


using namespace Utils;
#define DCF_PIN 2             // Connection pin to DCF 77 device
#define DCF_INTERRUPT 0         // Interrupt number associated with pin
time_t time;
DCF77 DCF = DCF77(DCF_PIN,DCF_INTERRUPT); 

void setup() {
  

}//end method; void setup

void loop() {
  
  time_t DCFtime = DCF.getTime(); 
  
  //create Matrix
  void cMatrix(time_t time){
    ledString[[9],[10]]; //Array [0-10],[0-9]
    
    //Second-Mode
    if (modeSc == true){
    
    Switch (second()){
      case 0:
        setTenZERO(); //First number (tenner)
        setSglZERO(); //Second number(single)
      case 1:
        setTenZERO();
        setSglONE();
      case 2:
        setTenZERO();
        setSglTWO();
      case 3:
        setTenZERO();
        setSglTHREE();
      case 4:
        setTenZERO();
        setSglFOUR();
      case 5:
        setTenZERO();
        setSglFIFE();
      case 6:
        setTenZERO();
        setSglSIX();
      case 7:
        setTenZERO();
        setSglSEVEN();
      case 8:
        setTenZERO();
        setSglEIGHT();           
      case 9:
        setTenZERO();
        setSglNEUN();
      case 10:
        setTenONE();
        setSglZERO();
      case 11:
        setTenONE();
        setSglONE();
      case 12:
        setTenONE();
        setSglTWO();
      case 13:
        setTenONE();
        setSglTHREE();
      case 14:
        setTenONE();
        setSglFOUR();
      case 15:
        setTenONE();
        setSglFIFE();
      case 16:
        setTenONE();
        setSglSIX();        
      case 17:
        setTenONE();
        setSglSEVEN();
      case 18:
        setTenONE();
        setSglEIGHT();
      case 19:
        setTenONE();
        setSglNINE();
      case 20:
        setTenTWO();
        setSglZERO();
      case 21:
        setTenTWO();
        setSglONE();
      case 22:
        setTenTWO();
        setSglTWO();
      case 23:
        setTenTWO();
        setSglTHREE();
      case 24:
        setTenTWO();
        setSglFOUR();
      case 25:
        setTenTWO();
        setSglFIFE();
      case 26:
        setTenTWO();
        setSglSIX();
      case 27:
        setTenTWO();
        setSglSEVEN();
      case 28:
        setTenTWO();
        setSglEIGHT();           
      case 29:
        setTenTWO();
        setSglNINE();
      case 30:
        setTenTHREE();
        setSglZERO();
      case 31:
        setTenTHREE();
        setSglONE();
      case 32:
        setTenTHREE();
        setSglTWO();
      case 33:
        setTenTHREE();
        setSglTHREE();
      case 34:
        setTenTHREE();
        setSglFOUR();
      case 35:
        setTenTHREE();
        setSglFIFE();
      case 36:
        setTenTHREE();
        setSglSIX();        
      case 37:
        setTenTHREE();
        setSglSEVEN();
      case 38:
        setTenTHREE();
        setSglEIGHT();
      case 39:
        setTenTHREE();
        setSglNINE();
      case 40:
        setTenFOUR();
        setSglZERO();
      case 41:
        setTenFOUR();
        setSglONE();
      case 42:
        setTenFOUR();
        setSglTWO();
      case 43:
        setTenFOUR();
        setSglTHREE();
      case 44:
        setTenFOUR();
        setSglFOUR();        
      case 45:
        setTenFOUR();
        setSglFIFE();
      case 46:
        setTenFOUR();
        setSglSIX();        
      case 47:
        setTenFOUR();
        setSglSEVEB();
      case 48:
        setTenFOUR();
        setSglEIGHT();
      case 49:
        setTenFOUR();
        setSglNINE();
      case 50:
        setTenFIFE();
        setSglZERO();
      case 51:
        setTenFIFE();
        setSglONE();
      case 52:
        setTenFIFE();
        setSglTWO();
      case 53:
        setTenFIFE();
        setSglTHREE();
      case 54:
        setTenFIFE();
        setSglFOUR();
      case 55:
        setTenFIFE();
        setSglFIFE();
      case 56:
        setTenFIFE();
        setSglSIX();
      case 57:
        setTenFIFE();
        setSglSEVEN();
      case 58:
        setTenFIFE();
        setSglEIGHT();           
      case 59:
        setTenFIFE();
        setSglNINE();
      case 60:
        setTenSIX();
        setSglZERO();
        
    }//end switch-case
  }//end if
      
    //Hour-Mode
    else if (modeHr == true) {
      
      //set "ES IST"
      setESIST();
      
      if (minute() < 5){

        //sets "UHR"
        setUHR();
        if (tm.hour == 1){
          setHrEINS();
          }//end if
        else {
        setHour(tm.hour());
        }//end else
        
        //xx:05
      else if (minute() < 10){
        setHour(tm.hour());
        setMinFUNF();
        setNACH();
        }//end else if
        
        //xx:10
      else if (minute() < 15){
        setHour(tm.hour());
        setMinZEHN();
        setNACH();
        }//end else if
        
        //xx:15
      else if (minute() < 20){
        setHour(tm.hour());
        setVIERTEL();
        setNACH();
        }//end else if
        
        //xx:20
      else if (minute() < 25){
        setHour(tm.hour() - 1);
        setMinZEHN();
        setVOR();
        }//end else if
        
        //xx:25
       else if (minute() < 30){
        setHour(tm.hour() - 1);
        setMinFUNF();
        setVOR();
        }//end else if
        
        //xx:30
       else if (minute() < 35){
        setHour(tm.hour() - 1);
        setHALB();
        }//end else if
        
        //xx:35
       else if (minute() < 40){
        setHour(tm.hour() - 1);
        setMinFUNF();
        setNACH();
        setHALB();
        }//end else if
        
        //xx:40
       else if (minute() < 45){
        setHour(tm.hour() - 1);
        setZEHN();
        setNACH();
        setHALB();
        }//end else if
        
        //xx:45
       else if (minute() < 50){
        setHour(tm.hour() - 1);
        setDREIVIERTEL();
        }//end else if
        
        //xx:50
      else if (minute() < 55){
        setHour(tm.hour() - 1);
        setMinZEHN();
        setVOR();
      }//end else if
      
        //xx:55
       else if (minute() < 60){
        setHour(tm.hour() - 1);
        setMinFUNF();
        setVOR();
        }//end else if
        
}//end else if

      
    //Temperature-Mode
else if (modeTr == true) {
    
    
    //Temperatures from 10 to 40 degrees
    
    //set temperature one time and refresh every full hour
  do while (minute() < 1){
    
    while (conversationDone() == false) //if there is no temperture read set 00
        {
        setTenONE();
        setSglZERO();
    }//end while

    int temperature = readTempOneShotInt();
        
    switch (temperature){;
      case 10:
        setTenONE();
        setSglZERO();
      case 11:
        setTenONE();
        setSglONE();
      case 12:
        setTenONE();
        setSglTWO();
      case 13:
        setTenONE();
        setSglTHREE();
      case 14:
        setTenONE();
        setSglFOUR();
      case 15:
        setTenONE();
        setSglFIFE();
      case 16:
        setTenONE();
        setSglSIX();        
      case 17:
        setTenONE();
        setSglSEVEN();
      case 18:
        setTenONE();
        setSglEIGHT();
      case 19:
        setTenONE();
        setSglNINE();
      case 20:
        setTenTWO();
        setSglZERO();
      case 21:
        setTenTWO();
        setSglONE();
      case 22:
        setTenTWO();
        setSglTWO();
      case 23:
        setTenTWO();
        setSglTHREE();
      case 24:
        setTenTWO();
        setSglFOUR();
      case 25:
        setTenTWO();
        setSglFIFE();
      case 26:
        setTenTWO();
        setSglSIX();
      case 27:
        setTenTWO();
        setSglSEVEN();
      case 28:
        setTenTWO();
        setSglEIGHT();           
      case 29:
        setTenTWO();
        setSglNINE();
      case 30:
        setTenTHREE();
        setSglZERO();
      case 31:
        setTenTHREE();
        setSglONE();
      case 32:
        setTenTHREE();
        setSglTWO();
      case 33:
        setTenTHREE();
        setSglTHREE();
      case 34:
        setTenTHREE();
        setSglFOUR();
      case 35:
        setTenTHREE();
        setSglFIFE();
      case 36:
        setTenTHREE();
        setSglSIX();        
      case 37:
        setTenTHREE();
        setSglSEVEN();
      case 38:
        setTenTHREE();
        setSglEIGHT();
      case 39:
        setTenTHREE();
        setSglNINE();
      case 40:
        setTenFOUR();
        setSglZERO();
    }//end switch-case
  }//end do-while loop
}//end else if
    
    
}//end method cmatrix

  
  void setHour(int printHr){
    switch (printHr){
      case 0: 
        setHrZWOLF();
        break;
       case 1:
        setHrEINS();
         break;
       case 2:
        setHrZWEI();
        break;
       case 3:
        setHrDREI();
        break;        
       case 4:
        setHrVIER();
        break;        
       case 5:
        setHrFUNF();
        break;
       case 6:
        setHrSECHS();
        break;
       case 7:
        setHrSIEBEN();
        break;
       case 8:
        setHrACHT();
        break;
       case 9:
        setHrNEUN();
        break;       
       case 10:
        setHrZEHN();
       case 11:
        setHrELF();
        break;
       case 12: 
        setHrZWOLF();
        break;
       case 13:
        setHrEINS();
         break;
       case 14:
        setHrZWEI();
        break;
       case 15:
        setHrDREI();
        break;        
       case 16:
        setHrVIER();
        break;        
       case 17:
        setHrFUNF();
        break;
       case 18:
        setHrSECHS();
        break;
       case 19:
        setHrSIEBEN();
        break;
       case 20:
        setHrACHT();
        break;
       case 21:
        setHrNEUN();
        break;       
       case 23:
        setHrZEHN();
       case 23:
        setHrELF();
        break;
       case 24: 
        setHrZWOLF();
        break;
     }//end switch 
  }//end method setHour
  
  void setMinute (int printMin){
    
    }//end method setMinute
    
//set LEDs  
    void setESIST (){
      ledString[[0],[0]] = 1;
      ledString[[0],[1]] = 1;
      ledString[[0],[2]] = 1;
      ledString[[0],[4]] = 1;
      ledString[[0],[5]] = 1;
    }//end method setESIST  
  void setVOR (){
      ledString[[3],[0]] = 1;
      ledString[[3],[1]] = 1;
      ledString[[3],[2]] = 1;
    }//end method setVOR
   void setNACH (){
        ledString[[3],[7]] = 1;
        ledString[[3],[8]] = 1;
        ledString[[3],[9]] = 1;
        ledString[[3],[10]] = 1;
    }//end method setNACH
   void setHALB (){
        ledString[[4],[0]] = 1;
        ledString[[4],[1]] = 1;
        ledString[[4],[2]] = 1;
        ledString[[4],[3]] = 1;
    }//end method setHALB
   void setUHR (){
        ledString[[9],[8]] = 1;
        ledString[[9],[9]] = 1;
        ledString[[9],[10]] = 1;
    }//end method setUHR
   void setVIERTEL(){
        ledString[[2],[4]] = 1;
        ledString[[2],[5]] = 1;
        ledString[[2],[6]] = 1;
        ledString[[2],[7]] = 1;
        ledString[[2],[8]] = 1;
        ledString[[2],[9]] = 1;
        ledString[[2],[10]] = 1;     
     }//end method setVIERTEL
    void setDREIVIERTEL (){
        setVIERTEL();
        ledString[[2],[0]] = 1;
        ledString[[2],[1]] = 1;
        ledString[[2],[2]] = 1;
    }//end method setDREIVIERTEL
   void setMinFUNF(){
        ledString[[0],[7]] = 1;
        ledString[[0],[8]] = 1;
        ledString[[0],[9]] = 1;
        ledString[[0],[10]] = 1;   
     }//end method setFUNF
   void setMinZEHN(){
        ledString[[1],[0]] = 1;
        ledString[[1],[1]] = 1;
        ledString[[1],[2]] = 1;
        ledString[[1],[3]] = 1;   
     }//end method setMinZEHN
   void setMinZWANZIG(){
        ledString[[1],[4]] = 1;
        ledString[[1],[5]] = 1;
        ledString[[1],[6]] = 1;
        ledString[[1],[7]] = 1;
        ledString[[1],[8]] = 1;
        ledString[[1],[9]] = 1;
        ledString[[1],[10]] = 1;     
     }//end method setMinZWANZIG
   void setHrEIN(){
        ledString[[5],[0]] = 1;
        ledString[[5],[1]] = 1;
        ledString[[5],[2]] = 1;
     }//end method setHrEIN
   void setHrEINS(){
       setHrEIN(); 
       ledString[[5],[3]] = 1;     
     }//end method setHrEINS     
   void setHrZWEI(){
        ledString[[5],[7]] = 1;
        ledString[[5],[8]] = 1;
        ledString[[5],[9]] = 1;
        ledString[[5],[10]] = 1;    
     }//end method setHrZWEI
